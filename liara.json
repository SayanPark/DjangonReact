{
  "app": "SZKblog",
  "port": 8000,
  "platform": "python",
  "build": {
    "location": "iran"
  },
  "python": {
    "version": "3.12"
  },
  "env": {
    "DJANGO_SETTINGS_MODULE": "backend.settings",
    "DEBUG": "False",
    "MEDIA_ROOT": "/tmp/media",
    "STATIC_ROOT": "/tmp/staticfiles",
    "DATABASE_DIR": "/tmp/database"
  },
  "disks": [
    {
      "name": "my-disk",
      "mountTo": "path/to/folder"
    }
  ]
}
